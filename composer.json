{
    "name": "nickwest/form-maker",
    "description": "Create Bulma compatible forms and tables directly from Eloquent Models and Collections",
    "authors": [
        {
            "name": "Nick West",
            "email": "nickwest@uw.edu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.2",
        "illuminate/database": "^9.2",
        "illuminate/validation": "^9.2",
        "nesbot/carbon": "^2.0",
        "maatwebsite/excel": "^3.1"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Nickwest\\FormMaker\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^7.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nickwest\\FormMaker\\FormMakerServiceProvider",
                "Nickwest\\FormMaker\\bulma\\FormMakerBulmaThemeServiceProvider"
            ]
        }
    }
}
