{
    "name": "nickwest/form-maker",
    "description": "Create Bulma compatible forms and tables directly from Eloquent Models and Collections",
    "authors": [
        {
            "name": "Nick West",
            "email": "nickwest@uw.edu"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/validation": "5.5.*",
        "nesbot/carbon": "^1.21",
        "maatwebsite/excel": "^2.1"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Nickwest\\FormMaker\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nickwest\\FormMaker\\FormMakerServiceProvider",
                "Nickwest\\FormMaker\\bulma\\FormMakerBulmaThemeServiceProvider"
            ]
        }
    }
}
